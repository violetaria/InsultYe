<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.getlosthere.insultye">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>

    <application
        android:name=".applications.InsultYeApplication"
        android:allowBackup="true"
        android:icon="@drawable/dragon_icon"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <meta-data
            android:name="AA_DB_NAME"
            android:value="insultye.db"/>
        <meta-data
            android:name="AA_DB_VERSION"
            android:value="2"/>
        <meta-data
            android:name="AA_MODELS"
            android:value="com.getlosthere.insultye.models.Word"/>
        <activity android:name=".activities.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".activities.EditActivity">
        </activity>
    </application>

</manifest>